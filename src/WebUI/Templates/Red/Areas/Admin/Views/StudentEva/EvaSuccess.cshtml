@using Framework.HtmlHelpers
@using WebUI.Areas.Admin.Models.Common
@using WebUI.Extensions
@using Domain
@{
	Layout = "~/Areas/Admin/Views/Shared/_AdminContent.cshtml";
	string redirectUrl = Request["redirectUrl"];
}
@section head {
}
@section body {
}

<div class="container">
	<div class="row">
		<div class="col-lg-4 col-md-offset-4 text-center">
			<h1 class="text-primary text" style="padding-bottom: 40px;">评价成功</h1>
			<div class="btn-group">
				@if (redirectUrl != null)
				{
					<a class="btn btn-default" href="@redirectUrl">返回继续评价</a>
				}
				else
				{
					<a class="btn btn-default" href="javascript:history.go(-1);">返回继续评价</a>
				}
			</div>
		</div>
	</div>
</div>

