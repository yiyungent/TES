@using Domain
@using WebUI.Extensions
@model NormType
@{
	Layout = "~/Areas/Admin/Views/Shared/_AdminContent.cshtml";
}

<div class="box box-primary">
	<div class="box-header with-border">
		<h3 class="box-title">评价类型 - 详细</h3>
	</div>
	<form>
		<div class="box-body">
			<div class="form-group">
				@Html.LabelFor(m => m.ID)
				@Html.TextBoxFor(m => m.ID, new { @class = "form-control", disabled = "disabled" })
			</div>
			<div class="form-group">
				@Html.LabelFor(m => m.Name)
				@Html.TextBoxFor(m => m.Name, new { @class = "form-control", disabled = "disabled" })
			</div>
			<div class="form-group">
				@Html.LabelFor(m => m.NormTypeCode)
				@Html.TextBoxFor(m => m.NormTypeCode, new { @class = "form-control", disabled = "disabled" })
			</div>
			<div class="form-group">
				@Html.LabelFor(m => m.Weight)
				@Html.TextBoxFor(m => m.Weight, new { @class = "form-control", disabled = "disabled" })
			</div>
		</div>
		<div class="box-footer">
			<div class="btn-group">
				<a class="btn btn-default" href="javascript:history.go(-1);">返回</a>
			</div>
		</div>
	</form>
</div>
